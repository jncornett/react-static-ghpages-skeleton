{
  "name": "react-static-ghpages-skeleton",
  "version": "1.0.0",
  "description": "Skeleton project with Webpack+React (and babel, bootstrap, font-awesome, karma, jasmine)",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf ./dist",
    "build:dev": "webpack",
    "build:prod": "webpack --env.production",
    "start:dev": "webpack-dev-server",
    "start:test": "karma start",
    "test": "karma start --single-run --browsers=PhantomJS",
    "release": "npm run test && npm run clean && npm run build:prod"
  },
  "keywords": [
    "webpack",
    "react",
    "skeleton"
  ],
  "author": "Joel Cornett <joel.cornett@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "karma-chrome-launcher": "^2.2.0",
    "webpack-dev-server": "^2.6.1"
  },
  "dependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "bootstrap": "^4.0.0-alpha.6",
    "cookie": "^0.3.1",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "font-awesome": "^4.7.0",
    "font-awesome-webpack": "0.0.5-beta.2",
    "html-webpack-plugin": "^2.30.1",
    "jasmine-core": "^2.7.0",
    "jquery": "^3.2.1",
    "json3": "^3.3.2",
    "karma": "^1.7.0",
    "karma-jasmine": "^1.1.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-webpack": "^2.0.4",
    "less": "^2.7.2",
    "node-sass": "^4.5.3",
    "phantomjs-prebuilt": "^2.1.15",
    "pinkie-promise": "^2.0.1",
    "popper.js": "^1.11.1",
    "react": "^15.6.1",
    "react-copy-to-clipboard": "^5.0.0",
    "react-dom": "^15.6.1",
    "react-fontawesome": "^1.6.1",
    "react-hot-loader": "^3.0.0-beta.7",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.4.1"
